version: '3'

services:
  previous_image:
    container_name: "{{ container_prefix }}-previous"
    image: "{{ previous_image }}"
    restart: always
    logging:
      driver: "journald"
  current_image:
    container_name: "{{ container_prefix }}-current"
    image: "{{ current_image }}"
    restart: always
    logging:
      driver: "journald"
